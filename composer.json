{
    "name": "krak/lava",
    "description": "Micro framework with macro potential",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/krakphp/lava",
    "keywords": [
        "psr-7",
        "http",
        "micro-framework",
        "console"
    ],
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krak\\Lava\\": "src"
        },
        "files": [
            "src/inc.php"
        ]
    },
    "require": {
        "evenement/evenement": "^2.0",
        "krak/auto-args": "^0.3.0",
        "krak/cargo": "^0.2.0",
        "krak/mw": "^0.5.0",
        "krak/mw-http": "0.3.x-dev",
        "nikic/fast-route": "^1.2",
        "nikic/iter": "^1.4",
        "symfony/console": "^3.2",
        "symfony/debug": "^3.2",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "krak/php-inc": "^0.1.3",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
